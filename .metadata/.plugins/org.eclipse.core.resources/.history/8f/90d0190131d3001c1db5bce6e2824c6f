package stepDefinitions;

import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class FilterTest {
	
	
	@Given("browser is open")
	public void user_lanched_the_site() {
	    System.out.println("User launch site");
	}

	@When("User scroll on the filter table and input a person name")
	public void user_scroll_on_the_filter_table_and_input_name() {
		System.out.println("User scroll site");
	}

	@Then("Product mapped on the user should be displayed")
	public void product_mapped_on_the_user_should_be_displayed() {
	   System.out.println("Display");
	}

}
